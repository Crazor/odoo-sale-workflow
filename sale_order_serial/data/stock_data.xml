<odoo>

    <record id="route_from_stock" model="stock.location.route">
        <field name="name">From Stock</field>
        <field name="company_id" />
        <field name="sequence">1</field>
        <field name="sale_selectable">True</field>
    </record>

    <record id="rule_from_stock" model="stock.rule">
        <field name="name">From Stock Rule</field>
        <field name="action">pull</field>
        <field name="picking_type_id" ref="stock.picking_type_out" />
        <field name="location_src_id" ref="stock.stock_location_stock" />
        <field name="location_id" ref="stock.stock_location_customers" />
        <field name="procure_method">make_to_stock</field>
        <field name="route_id" ref="route_from_stock" />
    </record>

</odoo>
